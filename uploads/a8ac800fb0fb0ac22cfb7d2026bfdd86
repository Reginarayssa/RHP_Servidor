select * from tab_fat;

select  count(*) from val_pro
where cd_tab_fat = 900;

insert into val_pro(CD_TAB_FAT, CD_PRO_FAT, DT_VIGENCIA, VL_HONORARIO, VL_OPERACIONAL, VL_TOTAL, CD_IMPORT, VL_SH, VL_SD, QT_PONTOS, QT_PONTOS_ANEST, SN_ATIVO, NM_USUARIO, DT_ATIVACAO, CD_SEQ_INTEGRA, DT_INTEGRA, CD_IMPORT_SIMPRO, CD_CONTRATO)
select 
900 CD_TAB_FAT, CD_PRO_FAT, DT_VIGENCIA, VL_HONORARIO, VL_OPERACIONAL, VL_TOTAL, CD_IMPORT, VL_SH, VL_SD, QT_PONTOS, QT_PONTOS_ANEST, SN_ATIVO, NM_USUARIO, DT_ATIVACAO, CD_SEQ_INTEGRA, DT_INTEGRA, CD_IMPORT_SIMPRO, CD_CONTRATO
from val_pro
where cd_tab_fat = 766;

begin 
COPIA_TABELA(900,766);
end;

INSERT INTO tab_fat values (1000, 'MEDICAMENTOS ONCOLOGICOS');
commit;

delete val_pro 
where cd_tab_fat = 900;

SELECT * FROM val_pro;

DROP TABLE tab_func;
drop table "DBAMV"."TAB_FUNC"
